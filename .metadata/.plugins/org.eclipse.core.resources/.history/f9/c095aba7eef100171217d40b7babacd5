package sort;

public class SortUtil {

	/**
	 * 冒泡排序 * @param data数据源
	 */
	public static void bubbleSort(int data[]) {

		for (int i = 0; i < data.length; i++) {

			for (int j = 0; j < data.length - 1 - i; j++) {

				if (data[j] < data[j + 1]) {
					// 交换数据
					int temp = data[j];
					data[j] = data[j + 1];
					data[j + 1] = temp;
					
//					data[j] = data[j]^data[j+1];
//					data[j+1]=data[j]^data[j+1];
//					data[j] = data[j]^data[j+1];
					
				}
			}
		}
	}
	
	
	
	
	
	
	
	

}
